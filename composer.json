{
    "name": "ionzile/ions",
    "type": "project",
    "description": "The Ion Framework.",
    "keywords": [
        "framework",
        "ionzile",
        "ions"
    ],
    "version": "3.0.0",
    "license": "MIT",
    "homepage": "https://ionzile.com",
    "authors": [
        {
            "name": "Ahmed Hamdi",
            "email": "taha.developer@outlook.com",
            "homepage": "https://ionzile.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ions\\": "core/",
            "API\\": "api/"
        },
        "classmap": [
            "bin/commands"
        ]
    },
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cartalyst/sentinel": "^v6.0",
        "edwinhoksberg/php-fcm": "^1.2",
        "gabordemooij/redbean": "dev-master",
        "illuminate/cache": "v9.0.2",
        "illuminate/console": "v9.0.2",
        "illuminate/database": "v9.0.2",
        "illuminate/events": "v9.0.2",
        "illuminate/filesystem": "v9.0.2",
        "illuminate/http": "v9.0.2",
        "illuminate/pagination": "v9.0.2",
        "illuminate/validation": "v9.0.2",
        "lcobucci/jwt": "^4.1.5",
        "monolog/monolog": "^2.3.5",
        "naucon/breadcrumbs": "^1.0.2",
        "peppeocchi/php-cron-scheduler": "^4.0",
        "smarty/smarty": "^v4.1.0",
        "symfony/config": "^v6.0.3",
        "symfony/mailer": "^6.0.3",
        "symfony/routing": "^v6.0.3",
        "symfony/security-csrf": "^v6.0.3",
        "symfony/translation": "^v6.0.3",
        "symfony/yaml": "^6.0.3",
        "twig/extra-bundle": "^v3.3.8",
        "twig/intl-extra": "^v3.3.5",
        "twig/twig": "^v3.3.8",
        "verot/class.upload.php": "^2.1.0",
        "vlucas/phpdotenv": "^v5.4.1"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.19.0",
        "filp/whoops": "^2.14.5",
        "ion-bazan/composer-diff": "^1.6",
        "pestphp/pest": "^1.21",
        "roave/security-advisories": "dev-latest",
        "spatie/ray": "^1.33.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
